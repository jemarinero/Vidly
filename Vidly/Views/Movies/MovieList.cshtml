@model IEnumerable<Vidly.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movie List</h2>
<table>
    <tr>
        <th>Movie</th>
    </tr>


    @foreach (var movies in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelMovie => movies.Name)
            </td>
        </tr>
    }

</table>